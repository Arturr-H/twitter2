  "query": "\n        SELECT posts.* FROM get_posts_default($1) posts\n            JOIN post_hashtags ph ON posts.id = ph.post_id\n            JOIN hashtags h ON ph.hashtag_id = h.id\n        WHERE\n            h.tag = $2;\n    ",
